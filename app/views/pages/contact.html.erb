<h1>Contact</h1>

<!-- <a href="/about">Go to about</a> -->
<%= link_to "Go to about", about_path %>

<form action="/contact" method="get">
  <input type="text"
    name="member"
    value="<%= params[:member] %>"
    placeholder="Who would you like to search for?">
  <input type="submit">
</form>

<ul>
  <% @members.each do |member| %>
    <li><%= member.capitalize %></li>
  <% end %>
</ul>